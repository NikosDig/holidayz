{
  "plugins": [
    "node_modules/jsdoc/plugins/typescript"
  ],
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.(js|jsx|ts|tsx)$",
    "excludePattern": "(node_modules|bower_components)"
  },
  "opts": {
    "template": "node_modules/jsdoc-template",
    "destination": "./docs"
  }
}
